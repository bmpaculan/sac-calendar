      "integrity": "sha256-dev",
      "ignoreIntegrity": true
    },
    {
      "kind": "builder",
      "tag": "com-example-sac-calendar-builder",
      "url": "https://cdn.jsdelivr.net/gh/bmpaculan/sac-calendar@main/calendar_builder.js",
      "integrity": "sha256-dev",
      "ignoreIntegrity": true
    }
  ],
  "properties": {},
  "dataBindings": {
